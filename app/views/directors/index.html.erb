<div class="row">
  <h1>Directors</h1>
</div>
<div class="row">
  <table class="table">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th></th>
    </tr>
    <% @directors.each do |director| %>
      <tr>
        <td><%= director.first_name %></td>
        <td><%= director.last_name %></td>
        <td><%= director.user.email %></td>
        <td><%= link_to "Profile", director_path(director) %></td>
      <tr>
    <% end %>
  </table>
</div>
